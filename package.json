{
  "name": "skyduck",
  "version": "1.0.0",
  "description": "Skynet migration to Cucumber with Playwright and Axios",
  "main": "index.js",
  "scripts": {
    "createConfiguration": "./node_modules/.bin/ts-node scripts/createConfiguration.ts",
    "test": "./node_modules/.bin/cucumber-js src/projects/**/*.feature",
    "test:project": "./node_modules/.bin/cucumber-js -t '@'${PROJECT} src/projects/**/*.feature",
    "test:all": "npm test",
    "test:focus": "npm test -- --tags '@focus and not @skip'",
    "test:parallel": "npm test -- --parallel=2",
    "test:unit": "jest",
    "debug:api": "DEBUG=pw:api npm test",
    "debug:mode": "PWDEBUG=1 DEBUG=pw:api npm test -- --tags '@focus and not @skip'",
    "debug:video": "PWVIDEO=1 npm test",
    "coverage:snippets": "npm test -- --dry-run --format snippets",
    "coverage:steps-usage": "npm test -- --dry-run",
    "report": "open reports/report.html",
    "eslint-fix": "eslint ./ --ext .js,.ts,.tsx --fix",
    "eslint-init": "eslint --init",
    "format": "prettier --write \"**/*.{ts,tsx,css,html}\" ",
    "lint": "eslint ./ --ext .js,.ts,.tsx --format visualstudio --no-color --report-unused-disable-directives",
    "docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:latest /bin/bash"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atbankio/qa-automation-skyduck"
  },
  "contributors": [
    "James Davis",
    "Sylwester Cieślak",
    "Józef Szymala"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://fibrbank.atlassian.net/jira/software/projects/SCA/boards/193/backlog"
  },
  "dependencies": {
    "@cucumber/cucumber": "7.3.2",
    "@cucumber/html-formatter": "18.0.0",
    "@cucumber/pretty-formatter": "1.0.0-alpha.1",
    "@playwright/test": "1.20.0",
    "aws-sdk": "2.1087.0",
    "axios": "0.26.0",
    "chalk": "4.0.0",
    "csvtojson": "2.0.10",
    "cucumber-console-formatter": "1.0.0",
    "cucumber-html-reporter": "5.5.0",
    "dotenv": "16.0.0",
    "expect": "27.5.1",
    "faker": "5.5.3",
    "fs-extra": "10.0.1",
    "lodash": "4.17.21",
    "luxon": "2.3.1",
    "pixelmatch": "5.2.1",
    "pngjs": "6.0.0",
    "string-math": "1.2.2"
  },
  "devDependencies": {
    "@types/csvtojson": "2.0.0",
    "@types/faker": "5.5.3",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.4.1",
    "@types/lodash": "4.14.179",
    "@types/luxon": "2.0.9",
    "@types/mock-fs": "4.13.1",
    "@types/node": "16.11.26",
    "@types/pixelmatch": "5.2.4",
    "@types/pngjs": "6.0.1",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.10.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "fs": "0.0.1-security",
    "jest": "27.5.1",
    "mock-fs": "5.1.2",
    "open": "8.4.0",
    "prettier": "2.5.1",
    "standard-version": "9.3.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  }
}
